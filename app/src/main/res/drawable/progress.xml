<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
        <rotate android:fromDegrees="270" android:toDegrees="270" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="ring" android:innerRadiusRatio="2.5" android:thicknessRatio="10">
                <solid android:color="#f4e19c"/>
            </shape>
        </rotate>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270" android:toDegrees="270" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="ring" android:innerRadiusRatio="2.5" android:thicknessRatio="10">
                <solid android:color="#3bd893"/>
            </shape>
        </rotate>
    </item>
</layer-list>
